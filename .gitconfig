  st = status
  br = branch
  logtree = "log --graph --oneline --decorate=full"
  ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
[init]
	templatedir = ~/.git-templates/git-secrets
[core]
	ignorecase = false
	editor = vim
[hub]
	protocol = https
[credential]
	helper = cache
[gpg]
	program = gpg2
[merge]
  conflictStyle = diff3
  ff = false
[pull]
  ff = only
[push]
  default = simple
[hub]
  protocol = https
[diff]
  algorithm = histogram
